<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama koin</th>
      <th>Jumlah koin</th>
      <th>Harga koin</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for( let i = 0; i < nominal.length; i++ ) { %>
    <tr>
      <td><%= i + 1 %> </td>
      <td><%= nominal[i].coinName %> </td>
      <td><%= nominal[i].coinQuantity %> </td>
      <td><%= nominal[i].price %> </td>
      <td>
        <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="post">
          <a href="/nominal/edit/<%= nominal[i]._id %> " class="btn btn-sm btn-warning">ubah</a>
          <button class="btn btn-sm btn-danger">hapus</button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
